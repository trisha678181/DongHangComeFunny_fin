<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
 <mapper namespace="AdminLogin">
 
 <select id="selectAdmin" resultType="ADMIN">
 	SELECT * FROM tb_admin
 	WHERE aid = #{aId}
 	AND apw = #{apw}
 </select>
 
 
 
 </mapper>